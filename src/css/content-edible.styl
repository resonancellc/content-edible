@import 'nib'
global-reset()
.content-edible
  outline 1px solid fuchsia

nav.edible-toolbar
  outline 1px solid #ccc
  a
    display inline-block
    padding 3px
    text-align center
    min-width 1em
    border 1px solid #cecece
    border-radius 4px
    &.active
      background-color #cecece
      color #fff
